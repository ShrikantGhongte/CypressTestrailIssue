  "scripts": {
    "format": "prettier --write \"server/**/*.ts\"",
    "format-ui": "prettier --write \"src/**/*.ts\"",
    "postversion": "git push && git push --tags",
    "extractversion": "node -e \"console.log('export const APP_VERSION = \\'' + process.env.npm_package_version +'\\';')\" > src/version.ts",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug 9339 --watch",
    "start:prod": "node dist/server/main",
    "bundle-analyze": "webpack-bundle-analyzer dist/client/stats.json",
    "pretest": "npm run config-staging && npm run extractversion",
    "test": "npm run test-ui && npm run test-server",
    "test-ui": "jest --config jest.config.app.js --runInBand",
    "test-server": "jest --config jest.config.server.js",
    "test:watch": "jest --watch",
    "test:cov": "npm run pretest && jest --coverage",
    "cypress:open": "cypress open --config-file cypress.local.config.ts",
    "cypress:run": "cypress run --headed --config-file cypress.local.config.ts --spec 'cypress/integration/Specs/Sanity/**/*' --browser",
    "cy:Bstack:Trail:Sanity": "browserstack-cypress run --headed --sync --spec 'cypress/integration/Specs/Sanity/**/*' --env",
    "trigger:Testrail:Report": "cypress run --config-file cypress.local.config.ts --spec 'cypress/integration/Specs/TestRail/Test_Rail_Report.ts'",
    "cy:MLPlatform:Sanity": "npm run cy:Bstack:Trail:Sanity %npm_config_base% & npm run trigger:Testrail:Report",
    "cy:tc:MLPlatform:Sanity": "npm run cy:Bstack:Trail:Sanity $npm_config_base ; npm run trigger:Testrail:Report",
    "cypress:regressionRun": "cypress run --headed --config-file cypress.local.config.ts --spec 'cypress/integration/Specs/Regression/**/*' --browser"
  },
